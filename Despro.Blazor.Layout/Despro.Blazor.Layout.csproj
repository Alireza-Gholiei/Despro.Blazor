<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Title>Despro Blazor Layout</Title>
		<PackageId>Despro.Blazor.Layout</PackageId>
		<Product>Despro.Blazor.Layout</Product>
		<PackageTags>Despro Blazor Layout</PackageTags>
		<Description>Layout Despro Blazor</Description>
		<Authors>Alireza Gholiei</Authors>
		<PackageIcon>Framework.png</PackageIcon>
		<Version>1.0.7</Version>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<None Include="Framework.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Despro.Blazor.Base\Despro.Blazor.Base.csproj" />
	</ItemGroup>

</Project>
