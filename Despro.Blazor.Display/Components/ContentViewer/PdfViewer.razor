@using Despro.Blazor.Base.Components
@inherits BaseComponent

@if (!string.IsNullOrWhiteSpace(objectURL))
{
    <div style="border: 2px solid black; width:100%;height:100%;">
        <iframe src="@(objectURL + UrlSuffix)" style="width:100%;height:100%; zoom:0.35;" @attributes="@UnmatchedParameters" />
    </div>
}